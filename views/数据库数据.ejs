<!DOCTYPE html>
<html>

    <head>
        <title>
            <%= title %>
        </title>
    </head>

    <body>
        <h1><%= title %> - 产品列表</h1>
        <table border="1" width="80%">
            <tr>
                <th>序号</th>
                <th>用户名称</th>
                <th>密码</th>
            </tr>
            <%users.forEach(function(user,index){%>
            <tr>
                <td>
                    <%=index+1%>
                </td>
                <td>
                    <%=user.用户名%>
                </td>
                <td>
                    <%=user.用户密码%>
                </td>
            </tr>
            <%});%>
        </table>

        <ul>
            <% for(var i=0; i<users.length; i++) {%>
            <li>
                <%=users[i].用户名%>
            </li>
            <% } %>
    </body>

</html>